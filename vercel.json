{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    { "src": "/bots", "dest": "bots.html" },
    { "src": "/session", "dest": "session.html" },
    { "src": "/alchat", "dest": "alchat.html" },
    { "src": "/url", "dest": "url.html" },
    { "src": "/tutorials", "dest": "tutorial.html" },
    { "src": "/music", "dest": "music.html" },
    { "src": "/myportfolio", "dest": "portfolio.html" },
    { "src": "/termux", "dest": "termux.html" },
    { "src": "/media", "dest": "media.html" },
    { "src": "/apks", "dest": "apk.html" },
    { "src": "/Baileys", "dest": "baileys.html" },
    { "src": "/registration", "dest": "register.html" },
    { "src": "/scanner", "dest": "pair.html" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}
